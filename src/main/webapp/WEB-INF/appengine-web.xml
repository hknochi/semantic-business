		/> -->
	<!-- <handler file="/html/timeout.html" error-code="timeout" /> -->
	<!-- </static-error-handlers> -->

	<inbound-services>
		<service>xmpp_message</service>
		<service>xmpp_presence</service>
	</inbound-services>


</appengine-web-app>
